# Application Settings
ASPNETCORE_ENVIRONMENT=Development
ASPNETCORE_URLS=http://+:8080
CORS_ORIGINS=*

# Database Settings
DB_HOST=postgres-vehicle
DB_PORT=5432
DB_NAME=vehicle_db
DB_USERNAME=postgres
DB_PASSWORD=postgres
# Connection string constructed from above variables
CONNECTION_STRING=Host=${DB_HOST};Port=${DB_PORT};Database=${DB_NAME};Username=${DB_USERNAME};Password=${DB_PASSWORD}
# PostgreSQL Container Settings
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=vehicle_db

# pgAdmin Settings
PGADMIN_EMAIL=admin@admin.com
PGADMIN_PASSWORD=admin123

# Host Port Mappings
HOST_APP_PORT=7001
HOST_DB_PORT=7433
HOST_PGADMIN_PORT=5050

# Keycloak Configuration
KEYCLOAK_AUTHORITY=http://localhost:8080/realms/fleet-fleet-management-app
KEYCLOAK_REQUIRE_HTTPS=false